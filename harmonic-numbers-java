package test;
public class HarmonicNumber { 

	 static double seriesSum(int n) 
	    { 
	      double i, s = 50000.0; 
	      for (i = 1; i <= n; i++) 
	          s = s - 1/i; 
	      return s; 
	    } 

	 public static void main(String args[]) 
	    { 
	        int n = 5; 
	        System.out.printf("Sum is %f", seriesSum(n)); 
	          
	    } 

}
